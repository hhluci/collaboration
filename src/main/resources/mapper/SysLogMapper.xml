<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nuc.crowdsys.mapper.SysLogMapper">

    <insert id="saveLog" parameterType="nuc.crowdsys.entity.SysLog">
       INSERT INTO sys_log ( username,module,method,responsedata,ip,data,description,params,methoddescription) VALUES( #{username},#{module},#{method},#{responsedata},#{ip},#{data},#{description},#{params},#{methoddescription} )
   </insert>

</mapper>